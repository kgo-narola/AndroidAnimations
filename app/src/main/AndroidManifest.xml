<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.narola.androidanimations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.harsh.recyclerviewdemo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="Recyclerview"
            android:name="com.harsh.recyclerviewdemo.RecyclerActivity"
            android:screenOrientation="fullSensor" />

        <activity
            android:label="ConfettiAnimation"
            android:name="com.alarmmanager.zpanimation.KonfettiAnimationActivity"
            android:screenOrientation="fullSensor" />

        <activity
            android:label="CompactToolbarAnimation"
            android:name="com.harsh.recyclerviewdemo.MainActivity2"
            android:screenOrientation="fullSensor" />

        <activity
            android:label="Single It"
            android:name="com.narola.androidanimations.recyclerviewmotionlayout.MotionLayoutActivity"
            android:screenOrientation="fullSensor" />

    </application>

</manifest>